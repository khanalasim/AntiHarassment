{
  "manifest_version": 2,

  "name": "Anti Harrassment",
  "description": "This extension flags Harrassment giving you a better internet experience",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["mystyles.css"],
      "js": ["jquery.js", "myscript.js"]
    }
  ],

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
   ]
}